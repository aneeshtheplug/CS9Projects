from AnimalShelter import AnimalShelter
from Animal import Animal

a = Animal("Tiger", '145', '15', 'Joe')
b = Animal("Tiger", '145', '15', 'Joe')
shelter = AnimalShelter()
shelter.addAnimal(a)
shelter.addAnimal(b)
shelter.removeSpecies('tiGeR') 

